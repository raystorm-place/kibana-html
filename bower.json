{
  "name": "kibana-html-plugin",
  "description": "Kibana Html Plugin Widget",
  "main": "index.js",
  "authors": [
    "Marius Kolkowski <marius.kolkowski@raystorm.place>"
  ],
  "license": "Apache 2.0",
  "keywords": [
    "kibana",
    "html",
    "plugin",
    "widget",
    "raystorm"
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "ace-builds": "1.2.3",
    "angular-ui-ace": "0.2.3"
  },
  "install": {
    "path": "public/deps",
    "sources": {
      "angular-ui-ace": "public/bower_components/angular-ui-ace/ui-ace.min.js",
      "ace-builds": [
        "public/bower_components/ace-builds/src-min-noconflict/theme-monokai.js",
        "public/bower_components/ace-builds/src-min-noconflict/mode-html.js",
        "public/bower_components/ace-builds/src-min-noconflict/ace.js"
      ]
    }
  }
}
